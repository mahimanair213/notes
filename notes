register.js 
$(document).ready(function () {
    $(".register-form").on("submit", function (e) {
        e.preventDefault(); // Prevent the default form submission

        let isValid = true; // Flag to check overall validity

        // Full Name Validation
        const name = $("#name").val().trim();
        if (name === "") {
            alert("Full Name is required.");
            isValid = false;
        }

        // Password Validation
        const password = $("#password").val().trim();
        if (password.length < 6) {
            alert("Password must be at least 6 characters long.");
            isValid = false;
        }

        // Email Validation
        const email = $("#email").val().trim();
        if (email === "") {
            alert("Email Address is required.");
            isValid = false;
        }

        // Phone Number Validation
        const phone = $("#phone").val().trim();
        if (phone === "" || phone.length !== 10) {
            alert("Phone Number must be 10 digits.");
            isValid = false;
        }

        // College Name Validation
        const college = $("#college").val().trim();
        if (college === "") {
            alert("College Name is required.");
            isValid = false;
        }

        // State Validation
        const state = $("#state").val();
        if (state === "") {
            alert("Please select your state.");
            isValid = false;
        }

        // Team Name Validation
        const team = $("#team").val().trim();
        if (team === "") {
            alert("Team Name is required.");
            isValid = false;
        }

        // Team Members Validation
        const teamMembers = $("#team-members").val();
        if (teamMembers === "") {
            alert("Please select the number of team members.");
            isValid = false;
        }

        // Problem Statement ID Validation
        const problemStatement = $("#problem-statement").val().trim();
        if (problemStatement === "") {
            alert("Problem Statement ID is required.");
            isValid = false;
        }

        // Problem Statement Edition Validation
        if (!$("input[name='edition']:checked").val()) {
            alert("Please select a Problem Statement Edition (Software/Hardware).");
            isValid = false;
        }

        // Final Check
        if (isValid) {
            alert("Submission Successful!");
            $(".register-form")[0].reset(); // Clear the form
        }
    });
});

